{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 14",
  
    "compilerOptions": {
      "lib": ["es2020"],
      "module": "commonjs",
      "target": "ES6",                
        "outDir": "dist/js",           
        "noEmitOnError": true,
        "noImplicitAny": true, 
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "target":["src/**/*"]
}  